config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/ar934x_wmac'
        option htmode 'HT20'
        option country 'DE'
        option txpower '4'

config wifi-iface
        option device 'radio0'
        option encryption 'none'
        option ssid 'Test_Nicht_Verwenden'
        option mode 'sta'
        option bssid 'A2:F3:C1:99:38:31'
        option network 'mesh'


